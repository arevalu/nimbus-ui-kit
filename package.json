{
  "name": "@nimbus/ui-kit",
  "version": "0.1.0",
  "description": "React Native UI Library",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "tsc": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --max-warnings=0 --cache",
    "prepare": "bob build && tsconfig-replace-paths --project tsconfig.json -s ./src -o ./lib/commonjs && tsconfig-replace-paths --project tsconfig.json -s ./src -o ./lib/module",
    "preinstall": "npx force-resolutions",
    "prepack": "bob build",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 6006",
    "start-storybook-web": "./node_modules/@storybook/react/bin/index.js",
    "build-storybook": "build-storybook -o docs-build",
    "chromatic": "npx chromatic"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/arevalu/nimbus-ui-kit",
  "author": "Luciana Arevalo <arevalo.lucianadv@gmail.com> (https://www.github.com/arevalu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arevalu/nimbus-ui-kit/issues"
  },
  "homepage": "https://github.com/arevalu/nimbus-ui-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/eslint-parser": "7.19.1",
    "@babel/runtime": "7.12.5",
    "@commitlint/config-conventional": "11.0.0",
    "@etchteam/storybook-addon-status": "4.2.1",
    "@mdx-js/loader": "2.1.1",
    "@mdx-js/react": "2.1.1",
    "@react-native-community/eslint-config": "2.0.0",
    "@release-it/conventional-changelog": "2.0.0",
    "@storybook/addon-actions": "6.5.6",
    "@storybook/addon-controls": "6.5.6",
    "@storybook/addon-docs": "6.5.15",
    "@storybook/addon-essentials": "6.5.6",
    "@storybook/addon-interactions": "6.5.6",
    "@storybook/addon-links": "6.5.6",
    "@storybook/addon-ondevice-actions": "6.0.1-beta.5",
    "@storybook/addon-ondevice-knobs": "6.0.0-alpha.0",
    "@storybook/addon-react-native-web": "0.0.18",
    "@storybook/addons": "6.5.15",
    "@storybook/builder-webpack5": "6.5.6",
    "@storybook/jest": "0.0.10",
    "@storybook/manager-webpack5": "6.5.6",
    "@storybook/react": "6.5.6",
    "@storybook/react-native": "6.0.1-beta.5",
    "@storybook/react-native-server": "6.0.0-alpha.0",
    "@storybook/testing-library": "0.0.11",
    "@storybook/theming": "6.5.15",
    "@svgr/webpack": "6.2.1",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.26",
    "@types/react-native": "0.66.15",
    "@types/styled-components": "5.1.22",
    "@types/styled-components-react-native": "5.1.3",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.3",
    "babel-plugin-react-native-web": "0.17.5",
    "chromatic": "6.5.4",
    "commitlint": "11.0.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-storybook": "0.5.12",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.66.2",
    "prettier": "2.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native-builder-bob": "0.18.0",
    "react-native-storybook-loader": "2.0.5",
    "react-native-web": "0.17.5",
    "react-refresh": "0.14.0",
    "remark-gfm": "3.0.1",
    "storybook-addon-performance": "0.16.1",
    "ts-loader": "9.2.6",
    "tsconfig-paths": "4.0.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "tsconfig-replace-paths": "0.0.11",
    "typescript": "4.5.5",
    "webpack": "5.40.0"
  },
  "overrides": {
    "meow": "8.1.2"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-native": ">=0.68.1",
    "styled-components": ">=5.2.0"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
